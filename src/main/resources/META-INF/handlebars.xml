<!-- Extensions that use classes only available when the Handlebars plugin is installed. -->
<idea-plugin version="2">
    <extensions defaultExtensionNs="com.intellij">
        <completion.contributor language="Handlebars" implementationClass="com.emberjs.hbs.HbsCompletionContributor"/>
        <psi.referenceContributor language="Handlebars" implementation="com.emberjs.hbs.HbsReferenceContributor"/>
        <lang.foldingBuilder language="Handlebars" implementationClass="com.emberjs.intl.HbsTranslationFoldingBuilder"/>
    </extensions>
</idea-plugin>
